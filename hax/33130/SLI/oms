  `cust_no` int(11) DEFAULT NULL,
  `or_date` date DEFAULT NULL,
  `total_amt` int(11) NOT NULL,
  PRIMARY KEY (`or_id`),
  KEY `fk_cust` (`cust_no`),
  CONSTRAINT `orders_ibfk_1` FOREIGN KEY (`cust_no`) REFERENCES `customer` (`cust_id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1 |
